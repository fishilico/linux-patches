From: Nicolas Iooss <nicolas.iooss_linux@m4x.org>
Date: Tue, 3 Mar 2015 15:13:48 +0800
Subject: [PATCH] {CONSTIFY} arch/x86/boot/tools/build: constify string
 parameters

---
 arch/x86/boot/tools/build.c | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/x86/boot/tools/build.c b/arch/x86/boot/tools/build.c
index cfd955cff525..8fe4c23faadc 100644
--- a/arch/x86/boot/tools/build.c
+++ b/arch/x86/boot/tools/build.c
@@ -143,7 +143,7 @@ static void usage(void)
 
 #ifdef CONFIG_EFI_STUB
 
-static void update_pecoff_section_header_fields(char *section_name, u32 vma, u32 size, u32 datasz, u32 offset)
+static void update_pecoff_section_header_fields(const char *section_name, u32 vma, u32 size, u32 datasz, u32 offset)
 {
 	unsigned int pe_header;
 	unsigned short num_sections;
@@ -179,7 +179,7 @@ static void update_pecoff_section_header_fields(char *section_name, u32 vma, u32
 	}
 }
 
-static void update_pecoff_section_header(char *section_name, u32 offset, u32 size)
+static void update_pecoff_section_header(const char *section_name, u32 offset, u32 size)
 {
 	update_pecoff_section_header_fields(section_name, offset, size, size, offset);
 }
@@ -304,7 +304,7 @@ static inline int reserve_pecoff_reloc_section(int c)
 	}								\
 } while (0)
 
-static void parse_zoffset(char *fname)
+static void parse_zoffset(const char *fname)
 {
 	FILE *file;
 	char *p;
-- 
