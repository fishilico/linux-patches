 drivers/bus/ti-sysc.c | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/drivers/bus/ti-sysc.c b/drivers/bus/ti-sysc.c
index 7cd2fd04b212..ee6c7f75bc3d 100644
--- a/drivers/bus/ti-sysc.c
+++ b/drivers/bus/ti-sysc.c
@@ -831,7 +831,7 @@ static int sysc_child_add_fck(struct sysc *ddata,
 		return -EEXIST;
 	}
 
-	l = clkdev_create(ddata->clocks[SYSC_FCK], name, dev_name(child));
+	l = clkdev_create(ddata->clocks[SYSC_FCK], name, "%s", dev_name(child));
 
 	return l ? 0 : -ENODEV;
 }
