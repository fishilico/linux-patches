+++ b/drivers/net/wireless/ath/ath10k/mac.c
@@ -5971,7 +5971,7 @@ static void ath10k_configure_filter(struct ieee80211_hw *hw,
 
 	mutex_lock(&ar->conf_mutex);
 
-	changed_flags &= SUPPORTED_FILTERS;
