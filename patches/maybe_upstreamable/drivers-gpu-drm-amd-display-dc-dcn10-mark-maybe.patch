---
 drivers/gpu/drm/amd/display/dc/dcn10/dcn10_link_encoder.c | 1 +
 1 file changed, 1 insertion(+)

diff --git a/drivers/gpu/drm/amd/display/dc/dcn10/dcn10_link_encoder.c b/drivers/gpu/drm/amd/display/dc/dcn10/dcn10_link_encoder.c
index c4287147b853..b9bb17f96ff7 100644
--- a/drivers/gpu/drm/amd/display/dc/dcn10/dcn10_link_encoder.c
+++ b/drivers/gpu/drm/amd/display/dc/dcn10/dcn10_link_encoder.c
@@ -1322,6 +1322,7 @@ void dcn10_link_encoder_update_mst_stream_allocation_table(
 		udelay(10);
 
 		value0 = REG_READ(DP_MSE_SAT_UPDATE);
+		(void)value0; /* Mark the variable as unused to please clang's -Wunused-but-set-variable */
 
 		REG_GET(DP_MSE_SAT_UPDATE,
 				DP_MSE_SAT_UPDATE, &value1);
