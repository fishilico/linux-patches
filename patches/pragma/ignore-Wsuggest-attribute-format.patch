 DEFINE_EVENT(brcms_msg_event, brcms_info,
 	TP_PROTO(struct va_format *vaf),
@@ -58,6 +62,9 @@ DEFINE_EVENT(brcms_msg_event, brcms_crit,
 	TP_ARGS(vaf)
 );
 
+__diag_push()
+__diag_ignore(GCC, 4, "-Wsuggest-attribute=format",
