diff --git a/kernel/panic.c b/kernel/panic.c
index 332736a72a58..4b3775e5930e 100644
--- a/kernel/panic.c
+++ b/kernel/panic.c
@@ -573,6 +573,11 @@ struct warn_args {
 	va_list args;
 };
 
+/* Ignore gcc warning about missing format attribute */
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 void __warn(const char *file, int line, void *caller, unsigned taint,
 	    struct pt_regs *regs, struct warn_args *args)
 {
@@ -615,6 +620,9 @@ void __warn(const char *file, int line, void *caller, unsigned taint,
 	/* Just a warning, don't kill lockdep. */
 	add_taint(taint, LOCKDEP_STILL_OK);
 }
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
 #ifndef __WARN_FLAGS
 void warn_slowpath_fmt(const char *file, int line, unsigned taint,
diff --git a/kernel/trace/bpf_trace.c b/kernel/trace/bpf_trace.c
index d2d7cf6cfe83..bab65ac9cc2c 100644
--- a/kernel/trace/bpf_trace.c
+++ b/kernel/trace/bpf_trace.c
@@ -377,6 +377,9 @@ static DEFINE_RAW_SPINLOCK(trace_printk_lock);
 #define MAX_TRACE_PRINTK_VARARGS	3
 #define BPF_TRACE_PRINTK_SIZE		1024
 
+__diag_push()
+__diag_ignore(GCC, 4, "-Wsuggest-attribute=format",
+	      "This function does not behave like printf even though it calls bstr_printf")
 BPF_CALL_5(bpf_trace_printk, char *, fmt, u32, fmt_size, u64, arg1,
 	   u64, arg2, u64, arg3)
 {
@@ -401,6 +404,7 @@ BPF_CALL_5(bpf_trace_printk, char *, fmt, u32, fmt_size, u64, arg1,
 
 	return ret;
 }
+__diag_pop()
 
 static const struct bpf_func_proto bpf_trace_printk_proto = {
 	.func		= bpf_trace_printk,
@@ -428,6 +432,9 @@ const struct bpf_func_proto *bpf_get_trace_printk_proto(void)
 
 #define MAX_SEQ_PRINTF_VARARGS		12
 
+__diag_push()
+__diag_ignore(GCC, 4, "-Wsuggest-attribute=format",
+	      "This function does not behave like printf even though it calls seq_bprintf")
 BPF_CALL_5(bpf_seq_printf, struct seq_file *, m, char *, fmt, u32, fmt_size,
 	   const void *, data, u32, data_len)
 {
@@ -449,6 +456,7 @@ BPF_CALL_5(bpf_seq_printf, struct seq_file *, m, char *, fmt, u32, fmt_size,
 
 	return seq_has_overflowed(m) ? -EOVERFLOW : 0;
 }
+__diag_pop()
 
 BTF_ID_LIST_SINGLE(btf_seq_file_ids, struct, seq_file)
 
