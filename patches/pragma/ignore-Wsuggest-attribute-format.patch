+#pragma GCC diagnostic pop
+#endif
 
 DEFINE_EVENT(iwlwifi_msg_event, iwlwifi_err,
 	TP_PROTO(struct va_format *vaf),
@@ -68,6 +75,10 @@ DEFINE_EVENT(iwlwifi_msg_event, iwlwifi_crit,
 	TP_ARGS(vaf)
 );
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 TRACE_EVENT(iwlwifi_dbg,
 	TP_PROTO(u32 level, bool in_interrupt, const char *function,
 		 struct va_format *vaf),
@@ -88,6 +99,9 @@ TRACE_EVENT(iwlwifi_dbg,
 	),
 	TP_printk("%s", __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 #endif /* __IWLWIFI_DEVICE_TRACE_MSG */
 
 #undef TRACE_INCLUDE_PATH
diff --git a/kernel/panic.c b/kernel/panic.c
index 8b2e002d52eb..a1f2091528fb 100644
--- a/kernel/panic.c
+++ b/kernel/panic.c
@@ -506,6 +506,11 @@ struct warn_args {
 	va_list args;
 };
 
+/* Ignore gcc warning about missing format attribute */
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 void __warn(const char *file, int line, void *caller, unsigned taint,
 	    struct pt_regs *regs, struct warn_args *args)
 {
@@ -550,6 +555,9 @@ void __warn(const char *file, int line, void *caller, unsigned taint,
 	/* Just a warning, don't kill lockdep. */
 	add_taint(taint, LOCKDEP_STILL_OK);
 }
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
 #ifdef WANT_WARN_ON_SLOWPATH
 void warn_slowpath_fmt(const char *file, int line, const char *fmt, ...)
diff --git a/lib/vsprintf.c b/lib/vsprintf.c
index 2a14733fa997..3a56db717f98 100644
--- a/lib/vsprintf.c
+++ b/lib/vsprintf.c
@@ -1934,8 +1934,15 @@ char *pointer(const char *fmt, char *buf, char *end, void *ptr,
 			va_list va;
 
 			va_copy(va, *((struct va_format *)ptr)->va);
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 			buf += vsnprintf(buf, end > buf ? end - buf : 0,
 					 ((struct va_format *)ptr)->fmt, va);
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 			va_end(va);
 			return buf;
 		}
-- 
