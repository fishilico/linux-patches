From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Nicolas Iooss <nicolas.iooss_linux@m4x.org>
Date: Sat, 4 Jul 2015 11:50:02 +0800
Subject: [PATCH] {PRAGMA} ignore -Wsuggest-attribute=format

---
 drivers/infiniband/hw/hfi1/trace_dbg.h             |  7 +++++++
 .../broadcom/brcm80211/brcmfmac/tracepoint.h       | 14 ++++++++++++++
 .../brcm80211/brcmsmac/brcms_trace_brcmsmac_msg.h  | 14 ++++++++++++++
 .../net/wireless/intel/iwlwifi/iwl-devtrace-msg.h  | 14 ++++++++++++++
 kernel/panic.c                                     |  8 ++++++++
 lib/vsprintf.c                                     |  7 +++++++
 6 files changed, 64 insertions(+)

diff --git a/drivers/infiniband/hw/hfi1/trace_dbg.h b/drivers/infiniband/hw/hfi1/trace_dbg.h
index e62171fb7379..e5c4b5981595 100644
--- a/drivers/infiniband/hw/hfi1/trace_dbg.h
+++ b/drivers/infiniband/hw/hfi1/trace_dbg.h
@@ -63,6 +63,10 @@
 
 #define MAX_MSG_LEN 512
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 DECLARE_EVENT_CLASS(hfi1_trace_template,
 		    TP_PROTO(const char *function, struct va_format *vaf),
 		    TP_ARGS(function, vaf),
@@ -80,6 +84,9 @@ DECLARE_EVENT_CLASS(hfi1_trace_template,
 			      __get_str(function),
 			      __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
 /*
  * It may be nice to macroize the __hfi1_trace but the va_* stuff requires an
diff --git a/drivers/net/wireless/broadcom/brcm80211/brcmfmac/tracepoint.h b/drivers/net/wireless/broadcom/brcm80211/brcmfmac/tracepoint.h
index 4d7d51f95716..50aa7d0249ed 100644
--- a/drivers/net/wireless/broadcom/brcm80211/brcmfmac/tracepoint.h
+++ b/drivers/net/wireless/broadcom/brcm80211/brcmfmac/tracepoint.h
@@ -39,6 +39,10 @@ static inline void trace_ ## name(proto) {}
 
 #define MAX_MSG_LEN		100
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 TRACE_EVENT(brcmf_err,
 	TP_PROTO(const char *func, struct va_format *vaf),
 	TP_ARGS(func, vaf),
@@ -54,7 +58,14 @@ TRACE_EVENT(brcmf_err,
 	),
 	TP_printk("%s: %s", __get_str(func), __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 TRACE_EVENT(brcmf_dbg,
 	TP_PROTO(u32 level, const char *func, struct va_format *vaf),
 	TP_ARGS(level, func, vaf),
@@ -72,6 +83,9 @@ TRACE_EVENT(brcmf_dbg,
 	),
 	TP_printk("%s: %s", __get_str(func), __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
 TRACE_EVENT(brcmf_hexdump,
 	TP_PROTO(void *data, size_t len),
diff --git a/drivers/net/wireless/broadcom/brcm80211/brcmsmac/brcms_trace_brcmsmac_msg.h b/drivers/net/wireless/broadcom/brcm80211/brcmsmac/brcms_trace_brcmsmac_msg.h
index 0e8a69ab909f..98c76f1be5f6 100644
--- a/drivers/net/wireless/broadcom/brcm80211/brcmsmac/brcms_trace_brcmsmac_msg.h
+++ b/drivers/net/wireless/broadcom/brcm80211/brcmsmac/brcms_trace_brcmsmac_msg.h
@@ -24,6 +24,10 @@
 
 #define MAX_MSG_LEN	100
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 DECLARE_EVENT_CLASS(brcms_msg_event,
 	TP_PROTO(struct va_format *vaf),
 	TP_ARGS(vaf),
@@ -37,6 +41,9 @@ DECLARE_EVENT_CLASS(brcms_msg_event,
 	),
 	TP_printk("%s", __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 
 DEFINE_EVENT(brcms_msg_event, brcms_info,
 	TP_PROTO(struct va_format *vaf),
@@ -58,6 +65,10 @@ DEFINE_EVENT(brcms_msg_event, brcms_crit,
 	TP_ARGS(vaf)
 );
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 TRACE_EVENT(brcms_dbg,
 	TP_PROTO(u32 level, const char *func, struct va_format *vaf),
 	TP_ARGS(level, func, vaf),
@@ -75,6 +86,9 @@ TRACE_EVENT(brcms_dbg,
 	),
 	TP_printk("%s: %s", __get_str(func), __get_str(msg))
 );
+#ifndef __clang__
+#pragma GCC diagnostic pop
+#endif
 #endif /* __TRACE_BRCMSMAC_MSG_H */
 
 #ifdef CONFIG_BRCM_TRACING
diff --git a/drivers/net/wireless/intel/iwlwifi/iwl-devtrace-msg.h b/drivers/net/wireless/intel/iwlwifi/iwl-devtrace-msg.h
index 5dfc9295a7e0..91b1d33cbb81 100644
--- a/drivers/net/wireless/intel/iwlwifi/iwl-devtrace-msg.h
+++ b/drivers/net/wireless/intel/iwlwifi/iwl-devtrace-msg.h
@@ -34,6 +34,10 @@
 
 #define MAX_MSG_LEN	110
 
+#ifndef __clang__
+#pragma GCC diagnostic push
+#pragma GCC diagnostic ignored "-Wsuggest-attribute=format"
+#endif
 DECLARE_EVENT_CLASS(iwlwifi_msg_event,
 	TP_PROTO(struct va_format *vaf),
 	TP_ARGS(vaf),
@@ -47,6 +51,9 @@ DECLARE_EVENT_CLASS(iwlwifi_msg_event,
 	),
 	TP_printk("%s", __get_str(msg))
 );
